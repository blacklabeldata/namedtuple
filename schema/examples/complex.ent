namespace users

import projects

// Location type
type Location {

  // Coordinates
  version 1 {
    required float64 latitude, longitude, altitude
  }
}

// User object
type User {

  // base user
  version 1 {
    required string uuid, username
    optional uint8 age
  }

  // 11/15/14
  version 2 {
      optional Location location
  }

  version 3 {
    optional friends = [$User]
  }
}
